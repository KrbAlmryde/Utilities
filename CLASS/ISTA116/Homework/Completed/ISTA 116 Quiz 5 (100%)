# Web Quiz 5
sleep=c(3,9,2,6,5)
error=c(5,1,11,6,7)
quiz5=data.frame(sleep,error)
----------------------------------------
# Find Mean for sleep and error
> mean(quiz5)
sleep error
    5     6
----------------------------------------
# Find Squared Deviations for each participant
> (sleep-mean(sleep))^2
     4 16  9  1  0
> (error-mean(error))^2
     1 25 25  0  1
----------------------------------------
# Cross-Product
> (sleep-mean(sleep))*(error-mean(error))
      2 -20 -15   0   0
----------------------------------------
# Sum of the Squared Deviations
> sum((sleep-mean(sleep))^2)
    30
> sum((error-mean(error))^2)
    52
----------------------------------------
#Find Variance
> var(sleep)
    7.5
> var(error)
    13
----------------------------------------
# Find Standard Deviation
> sd(sleep)
    2.738613
> sd(error)
    3.605551
----------------------------------------
# Sum of Cross-Product
> sum((sleep-mean(sleep))*(error-mean(error)))
    -33
----------------------------------------
# Pearson's r relating sleep to errors
> cor(sleep,error)
    -0.83551





----------------------------------------
# Now the Rank version of the above
rsleep=rank(c(3,9,2,6,5))
rerror=rank(c(5,1,11,6,7))
quiz5=data.frame(rsleep,rerror)
----------------------------------------
# Find Mean for rsleep and rerror
> mean(rsleep)
    3
> mean(rerror)
    3
----------------------------------------
# Find Squared Deviations for each participant
> (rsleep-mean(rsleep))^2
	1 4 4 1 0
> (rerror-mean(rerror))^2
	1 4 4 0 1
----------------------------------------
# Cross-Product
> (rsleep-mean(rsleep))*(rerror-mean(rerror))
    1 -4 -4  0  0
----------------------------------------
# Sum of the Squared Deviations
> sum((rsleep-mean(rsleep))^2)
	10
> sum((rerror-mean(rerror))^2)
	10
----------------------------------------
#Find Variance
> var(rsleep)
	2.5
> var(rerror)
	2.5
----------------------------------------
# Find Standard Deviation
> sd(rsleep)
	1.581139
> sd(rerror)
	1.581139
----------------------------------------
# Sum of Cross-Product
> sum((rsleep-mean(rsleep))*(rerror-mean(rerror)))
	-7
----------------------------------------
# Pearson's r relating rsleep to rerrors
> cor(rsleep,rerror)
	-0.7
----------------------------------------
